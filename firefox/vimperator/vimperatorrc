set hintchars=asdfghjkl;

" Use <C-i> to open
set editor="gvim -f"

command -nargs=1 -complete="custom,completion.tabgroup" -description="Switch to other panorama group" ps panorama switch <args>

command -nargs=1 -complete="custom,completion.tabgroup" -description="Add a new panorama group" pn panorama add <args>

command -nargs=1 -complete="custom,completion.tabgroup" -description="Push the current tab to a panorama group" -bang pp panorama push<bang> <args>
